<p id="notice"><%= notice %></p>

<h1>Job Post Skill Sets</h1>

<table>
  <thead>
    <tr>
      <th>Skill set</th>
      <th>Job post</th>
      <th>Skill level</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_post_skill_sets.each do |job_post_skill_set| %>
      <tr>
        <td><%= job_post_skill_set.skill_set %></td>
        <td><%= job_post_skill_set.job_post %></td>
        <td><%= job_post_skill_set.skill_level %></td>
        <td><%= link_to 'Show', job_post_skill_set %></td>
        <td><%= link_to 'Edit', edit_job_post_skill_set_path(job_post_skill_set) %></td>
        <td><%= link_to 'Destroy', job_post_skill_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job Post Skill Set', new_job_post_skill_set_path %>
