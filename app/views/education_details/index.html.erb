<p id="notice"><%= notice %></p>

<h1>Education Details</h1>

<table>
  <thead>
    <tr>
      <th>User account</th>
      <th>Certificate degree name</th>
      <th>Major</th>
      <th>Institute university name</th>
      <th>Starting date</th>
      <th>Completion date</th>
      <th>Percentage</th>
      <th>Cgpa</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @education_details.each do |education_detail| %>
      <tr>
        <td><%= education_detail.user_account %></td>
        <td><%= education_detail.certificate_degree_name %></td>
        <td><%= education_detail.major %></td>
        <td><%= education_detail.Institute_university_name %></td>
        <td><%= education_detail.starting_date %></td>
        <td><%= education_detail.completion_date %></td>
        <td><%= education_detail.percentage %></td>
        <td><%= education_detail.cgpa %></td>
        <td><%= link_to 'Show', education_detail %></td>
        <td><%= link_to 'Edit', edit_education_detail_path(education_detail) %></td>
        <td><%= link_to 'Destroy', education_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Education Detail', new_education_detail_path %>
