<p id="notice"><%= notice %></p>

<h1>Skill Sets</h1>

<table>
  <thead>
    <tr>
      <th>Skill set name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @skill_sets.each do |skill_set| %>
      <tr>
        <td><%= skill_set.skill_set_name %></td>
        <td><%= link_to 'Show', skill_set %></td>
        <td><%= link_to 'Edit', edit_skill_set_path(skill_set) %></td>
        <td><%= link_to 'Destroy', skill_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Skill Set', new_skill_set_path %>
