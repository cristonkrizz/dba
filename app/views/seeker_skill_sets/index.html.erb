<p id="notice"><%= notice %></p>

<h1>Seeker Skill Sets</h1>

<table>
  <thead>
    <tr>
      <th>User account</th>
      <th>Skill set</th>
      <th>Skill level</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seeker_skill_sets.each do |seeker_skill_set| %>
      <tr>
        <td><%= seeker_skill_set.user_account %></td>
        <td><%= seeker_skill_set.skill_set %></td>
        <td><%= seeker_skill_set.skill_level %></td>
        <td><%= link_to 'Show', seeker_skill_set %></td>
        <td><%= link_to 'Edit', edit_seeker_skill_set_path(seeker_skill_set) %></td>
        <td><%= link_to 'Destroy', seeker_skill_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seeker Skill Set', new_seeker_skill_set_path %>
